docker run --detach \
        --name fx-nginx \
        -p 443:443\
        -p 80:80 \
        -v $HOME/k8s/nginx/data:/usr/share/nginx/html:rw\
        -v $HOME/k8s/nginx/nginx.conf:/etc/nginx/nginx.conf/:rw\
        -v $HOME/k8s/nginx/conf.d/default.conf:/etc/nginx/conf.d/default.conf:rw\
        -v $HOME/k8s/nginx/logs:/var/log/nginx/:rw\
        -v $HOME/k8s/nginx/ssl:/ssl/:rw\
        -d nginx
